import java.util.Scanner;
public class matr{
    public static void main(String[] args){
         Scanner sc = new Scanner(System.in);
         int n = sc.nextInt();
         int m = sc.nextInt();
         for(int i =0;i<n;i++){
            for(int j =0:i<j;j++){
                System.out.print("*");
            }
            System.out.println();
         }
    }
}